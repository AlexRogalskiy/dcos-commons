{
  "assets": {
    "uris": {
      "jre-tar-gz": "{{jre-url}}",
      "libmesos-bundle-tar-gz": "{{libmesos-bundle-url}}",
      "scheduler-zip": "{{artifact-dir}}/couchdb-scheduler.zip",
      "executor-zip": "{{artifact-dir}}/executor.zip"
    },
    "container": {
      "docker": {
        "couchdb": "couchdb:2.1.1"
      }
    }
  },
  "images": {
    "icon-small": "https://s3-us-west-1.amazonaws.com/px-dcos/portworx-couchdb/img/couchdb-small.png",
    "icon-medium": "https://s3-us-west-1.amazonaws.com/px-dcos/portworx-couchdb/img/couchdb-medium.png",
    "icon-large": "https://s3-us-west-1.amazonaws.com/px-dcos/portworx-couchdb/img/couchdb-large.png"
  },
  "cli":{
    "binaries":{
      "darwin":{
        "x86-64":{
          "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-portworx-couchdb-darwin}}" } ],
          "kind":"executable",
          "url":"{{artifact-dir}}/dcos-portworx-couchdb-darwin"
        }
      },
      "linux":{
        "x86-64":{
          "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-portworx-couchdb-linux}}" } ],
          "kind":"executable",
          "url":"{{artifact-dir}}/dcos-portworx-couchdb-linux"
        }
      },
      "windows":{
        "x86-64":{
          "contentHash":[ { "algo":"sha256", "value":"{{sha256:dcos-portworx-couchdb.exe}}" } ],
          "kind":"executable",
          "url":"{{artifact-dir}}/dcos-portworx-couchdb.exe"
        }
      }
    }
  }
}
